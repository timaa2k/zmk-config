#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/ext_power.h>

/ {
	keymap {
		compatible = "zmk,keymap";

		default_layer {
/* QWERTY
 * ,-----------------------------------------.                    ,-----------------------------------------.
 * |   `  |   1  |   2  |   3  |   4  |   5  |                    |   6  |   7  |   8  |   9  |   0  |  =   |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * | ESC  |   Q  |   W  |   E  |   R  |   T  |                    |   -  |   Y  |   U  |   I  |   O  |  P   |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * | LCTRL|   A  |   S  |   D  |   F  |   G  |-------.    ,-------|   '  |   H  |   J  |   K  |   L  |  ;   |
 * |------+------+------+------+------+------|   [   |    |    ]  |------+------+------+------+------+------|
 * | LGUI |   Z  |   X  |   C  |   V  |   B  |-------|    |-------|   /  |   N  |   M  |   ,  |   .  |  |   |
 * `-----------------------------------------/       /     \      \-----------------------------------------'
 *                   | LAlt |RAISE |LShift| / Tab   /       \Enter \  |Space |BackSP|TOGGLE|
 *                   |      |      |      |/       /         \      \ |      |      |      |
 *                   `----------------------------'           '------''--------------------'
 */
			bindings = <
&kp GRAVE  &kp N1  &kp N2  &kp N3  &kp N4  &kp N5                        &kp N6     &kp N7  &kp N8  &kp N9     &kp N0   &kp EQUAL
&kp ESC    &kp Q   &kp W   &kp E   &kp R   &kp T                         &kp MINUS  &kp Y   &kp U   &kp I      &kp O    &kp P
&kp LCTRL  &kp A   &kp S   &kp D   &kp F   &kp G                         &kp SQT    &kp H   &kp J   &kp K      &kp L    &kp SEMI
&kp LGUI   &kp Z   &kp X   &kp C   &kp V   &kp B  &kp LBKT     &kp RBKT  &kp FSLH   &kp N   &kp M   &kp COMMA  &kp DOT  &kp PIPE
                     &kp LALT  &mo 2  &kp LSHFT  &kp TAB         &kp RET  &kp SPACE  &kp BSPC  &tog 1
			>;

			sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
		};

		lower_layer {
/* COLEMAK-DH
 * ,-----------------------------------------.                    ,-----------------------------------------.
 * |   `  |   1  |   2  |   3  |   4  |   5  |                    |   6  |   7  |   8  |   9  |   0  |  =   |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * | ESC  |   Q  |   W  |   F  |   P  |   B  |                    |   J  |   L  |   U  |   Y  |   ;  |  -   |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * | LCTRL|   A  |   R  |   S  |   T  |   G  |-------.    ,-------|   M  |   N  |   E  |   I  |   O  |  '   |
 * |------+------+------+------+------+------|   [   |    |    ]  |------+------+------+------+------+------|
 * | LGUI |   Z  |   X  |   C  |   D  |   V  |-------|    |-------|   K  |   H  |   ,  |   .  |   /  |  |   |
 * `-----------------------------------------/       /     \      \-----------------------------------------'
 *                   | LAlt |RAISE |LShift| / Tab   /       \Enter \  |Space |BackSP|TOGGLE|
 *                   |      |      |      |/       /         \      \ |      |      |      |
 *                   `----------------------------'           '------''--------------------'
 */
			bindings = <
&kp GRAVE  &kp N1  &kp N2  &kp N3  &kp N4  &kp N5                        &kp N6  &kp N7  &kp N8     &kp N9   &kp N0    &kp EQUAL
&kp ESC    &kp Q   &kp W   &kp F   &kp P   &kp B                         &kp J   &kp L   &kp U      &kp Y    &kp SEMI  &kp MINUS
&kp LCTRL  &kp A   &kp R   &kp S   &kp T   &kp G                         &kp M   &kp N   &kp E      &kp I    &kp O     &kp SQT
&kp LGUI   &kp Z   &kp X   &kp C   &kp D   &kp V  &kp LBKT     &kp RBKT  &kp K   &kp H   &kp COMMA  &kp DOT  &kp FSLH  &kp PIPE
                     &kp LALT  &mo 2  &kp LSHFT  &kp TAB         &kp RET  &kp SPACE  &kp BSPC  &tog 1
			>;

			sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
		};

		raise_layer {
/* SPECIAL
 * ,-----------------------------------------.                    ,-----------------------------------------.
 * |  F11 |  F1  |  F2  |  F3  |  F4  |  F5  |                    |  F6  |  F7  |  F8  |  F9  | F10  | F12  |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * |EP_ON |EP_OFF|EP_TOG|      |      |      |                    |   ^  |   &  | PgUp |   {  |   }  |   |  |
 * |------+------+------+------+------+------|                    |------+------+------+------+------+------|
 * |      |      |      | PgDn |      |      |-------.    ,-------|   ~  | Left | Down |  Up  | Right|   *  |
 * |------+------+------+------+------+------|       |    |       |------+------+------+------+------+------|
 * |BTCLR | BT1  | BT2  | BT3  |  BT4 | BT5  |-------|    |-------|   +  |   -  |   =  |   [  |   ]  |   \  |
 * `-----------------------------------------/       /     \      \-----------------------------------------'
 *                   |      |      |      | /       /       \      \  |      |      |      |
 *                   |      |      |      |/       /         \      \ |      |      |      |
 *                   `----------------------------'           '------''--------------------'
 */
			bindings = <
&kp F11           &kp F1             &kp F2             &kp F3  &kp F4  &kp F5                      &kp F6       &kp F7     &kp F8     &kp F9    &kp F10    &kp F12
&ext_power EP_ON  &ext_power EP_OFF  &ext_power EP_TOG  &trans  &trans  &trans                      &kp CARET    &kp AMPS   &kp PGUP   &kp LPAR  &kp RPAR   &kp PIPE
&trans            &trans             &trans        &trans       &trans  &trans                      &kp TILDE    &kp LEFT   &kp DOWN   &kp UP    &kp RIGHT  &kp KP_MULTIPLY
&bt BT_CLR  &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &trans    &trans  &kp KP_PLUS  &kp MINUS  &kp EQUAL  &kp LBKT  &kp RBKT   &kp BSLH
                                   &trans       &trans      &trans      &trans                     &trans    &trans      &trans    &trans
			>;

			sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
		};
	};
};
